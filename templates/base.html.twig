<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Aufwiderzen!{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('css/vendors/bootstrap.min.css') }}" rel="stylesheet" />
            <link href="{{ asset('css/vendors/bootstrap-grid.min.css') }}" rel="stylesheet" />
            <link href="{{ asset('css/vendors/bootstrap-reboot.min.css') }}" rel="stylesheet" />
            <link href="{{ asset('css/vendors/jquery-ui.min.css') }}" rel="stylesheet" />
            <link href="{{ asset('css/vendors/jquery-ui.structure.min.css') }}" rel="stylesheet" />
            <link href="{{ asset('css/vendors/jquery-ui.theme.min.css') }}" rel="stylesheet" />
        {% endblock %}
    </head>
